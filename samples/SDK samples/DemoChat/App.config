<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <section name="totalapi.log" type="TotalApi.Core.Api.FileLoggerConfiguration, TotalApi.Core" />
    </configSections>
    <totalapi.log>
        <files>
            <add file="con" level="15" />
        </files>
    </totalapi.log>

    <appSettings>
        <add key="serverProtocol" value="http" />
<!--
        <add key="serverHost" value="services.totalapi.io:1202" />
-->
        <add key="appKey" value="DevAppKey" />
    </appSettings>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>

  <runtime>

    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">

      <dependentAssembly>

        <assemblyIdentity name="Linq2Rest" publicKeyToken="73a6c4cf8c87a30e" culture="neutral" />

        <bindingRedirect oldVersion="0.0.0.0-4.0.1.0" newVersion="4.0.1.0" />

      </dependentAssembly>

    </assemblyBinding>

  </runtime>
</configuration>